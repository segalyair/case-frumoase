{
	"name": "case-frumoase",
	"version": "0.0.1",
	"scripts": {
		"dev": "vite dev",
		"build": "pnpm pagefind && vite build",
		"preview": "vite preview",
		"pagefind": "vite build --mode=pagefind && npx -y pagefind --site .vercel/output/static --output-path static/pagefind --verbose",
		"test": "playwright test",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"deploy:vercel": "node build.js && pnpm pagefind && vite build && vercel deploy --prebuilt --prod"
	},
	"devDependencies": {
		"@playwright/test": "1.41.2",
		"@poppanator/sveltekit-svg": "^4.2.1",
		"@sveltejs/adapter-auto": "^3.1.1",
		"@sveltejs/adapter-static": "^3.0.1",
		"@sveltejs/adapter-vercel": "^5.1.0",
		"@sveltejs/kit": "^2.5.0",
		"@sveltejs/vite-plugin-svelte": "^3.0.2",
		"@types/cookie": "^0.6.0",
		"@types/node": "^20.11.16",
		"@typescript-eslint/eslint-plugin": "^6.20.0",
		"@typescript-eslint/parser": "^6.20.0",
		"eslint": "^8.56.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"prettier": "^3.2.5",
		"prettier-plugin-svelte": "^3.1.2",
		"svelte": "^4.2.9",
		"svelte-check": "^3.6.3",
		"svelte-preprocess": "^5.1.3",
		"tslib": "^2.6.2",
		"typescript": "^5.3.3",
		"vite": "^5.0.12",
		"vite-imagetools": "^6.2.9"
	},
	"type": "module",
	"dependencies": {
		"@vercel/blob": "^0.20.0",
		"cheerio": "1.0.0-rc.12",
		"embla-carousel": "8.0.0-rc22",
		"embla-carousel-autoplay": "8.0.0-rc22",
		"express": "^4.18.2",
		"imagetools-core": "^6.0.4",
		"node-fetch": "^3.3.2",
		"sharp": "^0.33.2"
	}
}